<launch>

  <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen" clear_params="true">
    <rosparam file="$(find robot_nav)/config/robot/costmap_common_params2.yaml" command="load" ns="global_costmap" />
    <rosparam file="$(find robot_nav)/config/robot/costmap_common_params2.yaml" command="load" ns="local_costmap" />
    <rosparam file="$(find robot_nav)/config/robot/local_costmap_params1.yaml" command="load" />
    <rosparam file="$(find robot_nav)/config/robot/global_costmap_params1.yaml" command="load" />
    <rosparam file="$(find robot_nav)/config/robot/base_local_planner_params1.yaml" command="load" />
  </node>
  
</launch>
